{
	"name": "sublime-merge-integration",
	"displayName": "Sublime Merge Integration",
	"author": {
		"name": "Ben Mosher",
		"email": "me@benmosher.com",
		"url": "https://github.com/benmosher"
	},
	"publisher": "benmosher",
	"description": "Quickly warp to Sublime Merge for common Git tasks.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sublime-merge.blame"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sublime-merge.blame",
				"title": "Sublime Merge: Blame"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "sublime-merge.blame",
					"when": "resourceScheme == file"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}